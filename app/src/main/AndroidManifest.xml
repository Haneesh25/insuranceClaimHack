<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.insuranceclaimhack">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.InsuranceClaimHack"
        tools:targetApi="31">
        <activity
            android:name=".ExampleItem"
            android:exported="false" />
        <activity
            android:name=".claimViewer"
            android:exported="false"
            android:label="@string/title_activity_claim_viewer"
            android:theme="@style/Theme.InsuranceClaimHack.NoActionBar" />
        <activity
            android:name=".login"
            android:exported="false"
            android:label="@string/title_activity_login"
            android:theme="@style/Theme.InsuranceClaimHack.NoActionBar" />
        <activity
            android:name=".writeClaim"
            android:exported="false"
            android:label="@string/title_activity_write_claim"
            android:theme="@style/Theme.InsuranceClaimHack.NoActionBar" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.InsuranceClaimHack.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>